{
  "host": "APP_HOST",
  "port": "APP_PORT",
  "public": "../client/build/",
  "debug": "DEBUG",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "mysql": "DB_CONN",
  "authentication": {
    "secret": "AUTH_SECRET",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "username",
      "passwordField": "password"
    }
  }
}
